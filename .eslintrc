{
  "extends":["prettier"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "comma-dangle": ["error","never"],
    "prettier/prettier": "error",
    "no-unused-vars": [ "error",
    {
      "vars": "local",
      "args": "none"
    }],
  },
}
