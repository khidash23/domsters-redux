{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true,
    "optionalDependencies": true, "peerDependencies": true}],
    "comma-dangle": ["error","never"],
    "no-unused-vars": [ "error",
    {
      "vars": "local",
      "args": "none"
    }],
    "react/jsx-filename-extension": 0
  }
}
